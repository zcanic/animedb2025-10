{
  "version": 2,
  "builds": [
    {
      "src": "main_prisma_postgres.py",
      "use": "@vercel/python"
    },
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/main_prisma_postgres.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/$1"
    }
  ],
  "functions": {
    "main_prisma_postgres.py": {
      "includeFiles": "frontend/**"
    }
  }
}